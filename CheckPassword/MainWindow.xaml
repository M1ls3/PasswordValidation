<Window x:Class="CheckPassword.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CheckPassword"
        mc:Ignorable="d"
        Title="MainWindow" Height="691" Width="518">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Center" Height="44" Margin="0,164,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="418" FontSize="30" Visibility="Collapsed"/>
        <Label x:Name="label" Content="Pass:" HorizontalAlignment="Left" Margin="50,123,0,0" VerticalAlignment="Top" FontSize="25" Height="41"/>
        <Button x:Name="button" Content="Sign Up" HorizontalAlignment="Center" VerticalAlignment="Center" Height="59" Width="418" FontSize="36" FontWeight="Bold" Click="button_Click"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Center" Margin="0,164,0,0" VerticalAlignment="Top" Width="418" Height="44" FontSize="30"/>
        <CheckBox x:Name="checkBox" Content="" HorizontalAlignment="Left" Margin="448,138,0,0" VerticalAlignment="Top" Checked="checkBox_Checked" Unchecked="checkBox_Unchecked"/>

    </Grid>
</Window>
